    "prepack": "npm run build",
    "test": "vitest"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.2",
    "buffer": "^6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/ic-js.git",
    "directory": "packages/nns"
  },
  "bugs": {
    "url": "https://github.com/dfinity/ic-js"
  },
  "keywords": [
    "internet computer",
    "internet-computer",
    "ic",
    "dfinity",
    "dfx",
    "canister",
    "candid",
    "motoko",
    "javascript",
    "typescript",
    "blockchain",
    "crypto",
    "nns",
    "network nervous system",
    "network-nervous-system"
  ],
  "peerDependencies": {
    "@dfinity/agent": "^3.0.0",
    "@dfinity/candid": "^3.0.0",
    "@dfinity/ledger-icp": "^4",
    "@dfinity/principal": "^3.0.0",
    "@dfinity/utils": "^2.13.0"
  }
