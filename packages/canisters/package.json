{
  "name": "@icp-sdk/canisters",
  "version": "1.0.0",
  "description": "A modular library for interacting with canisters on the Internet Computer.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.mjs"
    },
    "./ckbtc": {
      "types": "./ckbtc/index.d.ts",
      "import": "./ckbtc/index.js",
      "require": "./ckbtc/index.mjs"
    },
    "./cketh": {
      "types": "./cketh/index.d.ts",
      "import": "./cketh/index.js",
      "require": "./cketh/index.mjs"
    },
    "./cmc": {
      "types": "./cmc/index.d.ts",
      "import": "./cmc/index.js",
      "require": "./cmc/index.mjs"
    },
    "./ic-management": {
      "types": "./ic-management/index.d.ts",
      "import": "./ic-management/index.js",
      "require": "./ic-management/index.mjs"
    },
    "./ledger/icp": {
      "types": "./ledger/icp/index.d.ts",
      "import": "./ledger/icp/index.js",
      "require": "./ledger/icp/index.mjs"
    },
    "./ledger/icrc": {
      "types": "./ledger/icrc/index.d.ts",
      "import": "./ledger/icrc/index.js",
      "require": "./ledger/icrc/index.mjs"
    },
    "./nns": {
      "types": "./nns/index.d.ts",
      "import": "./nns/index.js",
      "require": "./nns/index.mjs"
    },
    "./sns": {
      "types": "./sns/index.d.ts",
      "import": "./sns/index.js",
      "require": "./sns/index.mjs"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "**/*.js",
    "**/*.js.map",
    "**/*.mjs",
    "**/*.mjs.map",
    "**/*.d.ts",
    "**/*.d.ts.map"
  ],
  "scripts": {
    "clean": "git ls-files --others --ignored --exclude-standard | grep -v \"LICENSE\" | xargs rm -rf",
    "prebuild": "node prebuild.mjs",
    "build": "npm run clean && npm run prebuild && tsc --noEmit && node esbuild.mjs",
    "prepack": "npm run build",
    "pretest": "npm run build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/ic-js.git",
    "directory": "packages/canisters"
  },
  "bugs": {
    "url": "https://github.com/dfinity/ic-js"
  },
  "keywords": [
    "internet computer",
    "icp",
    "icrc",
    "dfinity",
    "canister",
    "token",
    "ledger",
    "ckbtc",
    "bitcoin",
    "cketh",
    "ethereum",
    "minter",
    "nns",
    "sns"
  ],
  "homepage": "https://github.com/dfinity/ic-js#readme",
  "dependencies": {
    "@dfinity/ckbtc": "^4",
    "@dfinity/cketh": "^4",
    "@dfinity/cmc": "^6",
    "@dfinity/ic-management": "^7",
    "@dfinity/ledger-icp": "^6",
    "@dfinity/ledger-icrc": "^4",
    "@dfinity/nns": "^10",
    "@dfinity/sns": "^4",
    "@noble/hashes": "^1.8.0",
    "base58-js": "^3.0.3",
    "bech32": "^2.0.0"
  },
  "peerDependencies": {
    "@dfinity/utils": "^3",
    "@icp-sdk/core": "^4"
  }
}
