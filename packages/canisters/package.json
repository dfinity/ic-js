{
  "name": "@icp-sdk/canisters",
  "version": "1.1.1",
  "description": "A modular library for interacting with canisters on the Internet Computer.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.mjs"
    },
    "./ckbtc": {
      "types": "./ckbtc/index.d.ts",
      "import": "./ckbtc/index.js",
      "require": "./ckbtc/index.mjs"
    },
    "./cketh": {
      "types": "./cketh/index.d.ts",
      "import": "./cketh/index.js",
      "require": "./cketh/index.mjs"
    },
    "./cmc": {
      "types": "./cmc/index.d.ts",
      "import": "./cmc/index.js",
      "require": "./cmc/index.mjs"
    },
    "./ic-management": {
      "types": "./ic-management/index.d.ts",
      "import": "./ic-management/index.js",
      "require": "./ic-management/index.mjs"
    },
    "./ledger/icp": {
      "types": "./ledger/icp/index.d.ts",
      "import": "./ledger/icp/index.js",
      "require": "./ledger/icp/index.mjs"
    },
    "./ledger/icrc": {
      "types": "./ledger/icrc/index.d.ts",
      "import": "./ledger/icrc/index.js",
      "require": "./ledger/icrc/index.mjs"
    },
    "./nns": {
      "types": "./nns/index.d.ts",
      "import": "./nns/index.js",
      "require": "./nns/index.mjs"
    },
    "./sns": {
      "types": "./sns/index.d.ts",
      "import": "./sns/index.js",
      "require": "./sns/index.mjs"
    }
  },
  "scripts": {
    "clean": "git ls-files --others --ignored --exclude-standard | grep -v \"LICENSE\" | xargs rm -rf",
    "ts-declaration": "tsc --emitDeclarationOnly --outDir .",
    "build": "npm run clean && node esbuild.mjs && npm run ts-declaration",
    "prepack": "npm run build",
    "pretest": "npm run build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/icp-js-canisters.git",
    "directory": "packages/canisters"
  },
  "bugs": {
    "url": "https://github.com/dfinity/icp-js-canisters"
  },
  "keywords": [
    "internet computer",
    "icp",
    "icrc",
    "dfinity",
    "canister",
    "token",
    "ledger",
    "ckbtc",
    "bitcoin",
    "cketh",
    "ethereum",
    "minter",
    "nns",
    "sns"
  ],
  "homepage": "https://github.com/dfinity/icp-js-canisters#readme",
  "peerDependencies": {
    "@dfinity/ckbtc": "^6",
    "@dfinity/cketh": "^6",
    "@dfinity/cmc": "^8",
    "@dfinity/ic-management": "^9",
    "@dfinity/ledger-icp": "^8",
    "@dfinity/ledger-icrc": "^6",
    "@dfinity/nns": "^11",
    "@dfinity/sns": "^6",
    "@dfinity/utils": "^4",
    "@icp-sdk/core": "^4"
  }
}
